body {
  font-family: arial, sans-serif;
  font-size: 83%;
  background: white;
  margin: 1.5em;
}

h1 {
  background: #FFF4C2;
  font-size: 135%;
  padding: 0.1em 0 0.1em 3px;
  border-top: 1px solid #FFCC33;
  margin-bottom:0.5em;
}

#view-source {
  text-align:right;
  margin-bottom:1em;
}

li {
  margin-top:1em;
}

.error {
  color: red;
}

.tableheader {
  background:#eee;
}

#listTable .tableheader {
  font-weight: bold;
}

table#listTable, table.foreignTable {
  margin: 0;
  padding: 0;
  border: 0;
  border-collapse: collapse;
}

tr.blankRow {
  empty-cells:show;
  line-height: 20px;
  height: 20px;
}

td.label, td.foreignLabel, td.foreignTableLabel {
  text-align: right;
  padding: .1em .4em .1em .4em;
}

td.foreignLabel {
  vertical-align: middle;
}

td.foreignTableLabel {
  vertical-align: top;
}

table#listTable thead th, table.foreignTable thead th, table.foreignTable tbody td, table#listTable tbody td {
  margin: 0;
  padding: .1em .4em .1em .4em;
  text-align: left;
  vertical-align: center;
  border-color: #D8D8D8;
  border-width: 1px 0px 1px 0px;
  border-style: solid;
}

'
